{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "outputDirectory": "dist",
  "buildCommand": "bun run build",
  "devCommand": "bun dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "runtime": "vercel/node@5.1.6"
    }
  }
}
